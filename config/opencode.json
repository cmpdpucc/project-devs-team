{
  "theme": "default",
  "model": "google/gemini-2.0-flash-lite-preview-02-05:free",
  "permission": {
    "bash": "ask",
    "edit": "allow",
    "read": "allow"
  },
  "tui": {
    "enabled": true,
    "diff_style": "auto"
  },
  "watcher": {
    "enabled": true,
    "ignore": [
      "node_modules/**",
      ".git/**",
      "dist/**",
      ".env*"
    ]
  },
  "rules": [
    ".agent/rules/GEMINI.md",
    ".agent/ARCHITECTURE.md"
  ]
}