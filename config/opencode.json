{
  "theme": "default",
  "model": "opencode/kimi-k2.5-free",
  "fallback_models": [
    "opencode/minimax-m2.5-free",
    "opencode/glm-5-free",
    "opencode/big-pickle",
    "opencode/gpt-5-nano"
  ],
  "permission": {
    "bash": "ask",
    "edit": "allow",
    "read": "allow"
  },
  "tui": {
    "enabled": true,
    "diff_style": "auto"
  },
  "watcher": {
    "enabled": true,
    "ignore": [
      "node_modules/**",
      ".git/**",
      "dist/**",
      ".env*"
    ]
  },
  "rules": [
    ".agent/rules/GEMINI.md",
    ".agent/ARCHITECTURE.md"
  ]
}